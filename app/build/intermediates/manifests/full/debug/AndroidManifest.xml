    <uses-permission android:name="com.app.hotgirlforbigo.permission.C2D_MESSAGE" />

    <application
        android:name="com.app.hotgirlforbigo.Player.DemoApplication"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/myTheme" >
        <activity
            android:name="com.app.hotgirlforbigo.MainActivity"
            android:configChanges="orientation"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.app.hotgirlforbigo.Activitys.SearchResultsActivity"
            android:configChanges="orientation"
            android:parentActivityName="com.app.hotgirlforbigo.MainActivity"
            android:theme="@style/AppTheme" >
            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />
        </activity>
        <activity
            android:name="com.app.hotgirlforbigo.Activitys.Splash_screen"
            android:screenOrientation="portrait"
            android:theme="@style/myTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.app.hotgirlforbigo.Activitys.PlayerActivity" />

        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="bafc626ef31c6b132320fbfb402cf45c01ad6e38" />

